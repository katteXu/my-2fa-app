{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "cloudflare",
  "mainBinaryName": "Cloudflare",
  "version": "0.1.0",
  "identifier": "com.cloudflare.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Cloudflare Net",
        "width": 414,
        "height": 736,
        "resizable": false,
        "fullscreen": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "externalBin": [
      "bin/ntpclient"
    ],
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ],
    "macOS": {
      "dmg": {
        "background": "assets/background.png",
        "appPosition": {
          "x": 160,
          "y": 150
        },
        "applicationFolderPosition": {
          "x": 460,
          "y": 150
        },
        "windowSize": {
          "height": 365,
          "width": 646
        }
      },
      "files": {},
      "hardenedRuntime": true,
      "minimumSystemVersion": "10.13",
      "signingIdentity": "B18A098290B0AF705DFD7CAB1E291E455A75819F"
    }
  }
}